<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jxaummd.agritureassistant">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.jxaummd.module_library.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".User.LoginUser"/>
        <activity android:name=".User.Userlable">
            <intent-filter>
                <action  android:name="app_user_lable"/>
            </intent-filter>
        </activity>

        <activity android:name=".User.SubmitMQ"/>
        <activity android:name=".question.SubmitMyMood"/>
    </application>

</manifest>